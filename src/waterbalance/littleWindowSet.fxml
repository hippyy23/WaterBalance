<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="waterbalance.trackerScreenController">
   <children>
      <Button fx:id="okSetButton" layoutX="36.0" layoutY="87.0" mnemonicParsing="false" onAction="#okSetButton" prefHeight="40.0" prefWidth="60.0" stylesheets="@Buttons.css" text="OK" textFill="#ededed">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="51.0" layoutY="14.0" prefHeight="25.0" prefWidth="149.0" text="SET YOUR DAILY GOAL!" textFill="#0a0000">
         <font>
            <Font name="Tahoma" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldSet" alignment="CENTER" layoutX="36.0" layoutY="39.0" prefHeight="33.0" prefWidth="179.0" promptText="IN MILLILITERS" style="-fx-background-radius: 50PX;">
         <font>
            <Font name="Tahoma" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="cancelSetButton" layoutX="125.0" layoutY="87.0" mnemonicParsing="false" onAction="#calcelSetButton" prefHeight="25.0" prefWidth="87.0" stylesheets="@Buttons.css" text="CANCEL" textFill="#ededed">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
